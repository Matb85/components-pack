{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~": [
        "./components"
      ],
      "~/*": [
        "./components/*"
      ],
      "@": [
        "./src"
      ],
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"]
}
