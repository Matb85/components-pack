{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~": [
        "./components"
      ],
      "~/*": [
        "./components/*"
      ],
      "@": [
        "./src"
      ],
      "@/*": [
        "./src/*"
      ]
    },
    "jsx": "preserve"
  },
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ]
}