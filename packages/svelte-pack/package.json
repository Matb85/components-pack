{
  "name": "@matb85/svelte-pack",
  "description": "UI components for Svelte 5",
  "version": "5.1.0",
  "author": "Mateusz Bis <mateuszbis85@gmail.com> (https://matb85.github.io/)",
  "license": "ISC",
  "private": false,
  "type": "module",
  "scripts": {
    "build": "vite build && svelte-package -i ./components",
    "dev": "vite dev --port 3000"
  },
  "peerDependencies": {
    "svelte": "^5.20.5"
  },
  "dependencies": {
    "@matb85/base-pack": "workspace:^5.1.0",
    "modular-slider": "^1.0.3"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.3.10",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "svelte": "^5.20.5",
    "typescript": "^5.8.2",
    "vite": "^6.2.0"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "svelte": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./Map.svelte": {
      "types": "./dist/Map.svelte.d.ts",
      "svelte": "./dist/Map.svelte",
      "import": "./dist/Map.svelte"
    },
    "./Photo.svelte": {
      "types": "./dist/Photo.svelte.d.ts",
      "svelte": "./dist/Photo.svelte",
      "import": "./dist/Photo.svelte"
    },
    "./PhotoViewer.svelte": {
      "types": "./dist/PhotoViewer.svelte.d.ts",
      "svelte": "./dist/PhotoViewer.svelte",
      "import": "./dist/PhotoViewer.svelte"
    },
    "./PhotoMultiViewer.svelte": {
      "types": "./dist/PhotoMultiViewer.svelte.d.ts",
      "svelte": "./dist/PhotoMultiViewer.svelte",
      "import": "./dist/PhotoMultiViewer.svelte"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Matb85/components-pack"
  },
  "bugs": {
    "url": "https://github.com/Matb85/components-pack/issues"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "svelte",
    "components",
    "ui",
    "svelte-components",
    "lightbox",
    "lighbox-svelte",
    "strapi",
    "strapi-svelte"
  ]
}
